{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "3",
    "flow-bin": "^0.56.0",
    "moment": "^2.19.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "pusher-js": "^4.2.1",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-datetime": "^2.10.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-select": "^1.0.0-rc.10",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; npm run build-css && react-scripts build'",
    "build:testing": "REACT_APP_ENV=testing npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "test": "react-scripts test --env=jsdom",
    "test:staged": "cross-env CI=true npm run test",
    "eject": "react-scripts eject",
    "flow": "flow",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepush-msg": "echo 'Pre-push checks...' && exit 0",
    "lint-staged": "lint-staged",
    "husky": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "flow",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-msg && npm run lint-staged",
      "pre-push": "npm run prepush-msg && npm run test"
    }
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "husky": "^0.15.0-rc.12",
    "lint-staged": "^7.0.0"
  }
}
